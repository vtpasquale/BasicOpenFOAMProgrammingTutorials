{
    patch1
    {
        type            fixedValue;
        value           uniform 0;
    }
    patch2
    {
        type            fixedValue;
        value           uniform 0;
    }
    patch3
    {
        type            fixedValue;
        value           uniform 0;
    }
    patch4
    {
        type            fixedValue;
        value           uniform 0;
    }
    emptyTop
    {
        type            empty;
    }
    emptyBottom
    {
        type            empty;
    }
